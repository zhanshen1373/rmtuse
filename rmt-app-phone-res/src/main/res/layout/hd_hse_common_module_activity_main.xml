    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/hd_hse_common_bule"
    android:paddingTop="@dimen/padding_medium"
    android:paddingRight="@dimen/padding_medium"
    android:paddingBottom="@dimen/padding_medium"
    android:orientation="horizontal" >
    <com.hd.hse.common.component.custom.NavigationBar
        android:id="@+id/hd_hse_common_module_navigation"
        android:layout_width="215dip"
        android:layout_height="match_parent" />
    <FrameLayout
        android:id="@+id/hd_hse_common_module_content_fl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/padding_medium"
        android:background="@drawable/hd_hse_common_corner_back" />
</LinearLayout>