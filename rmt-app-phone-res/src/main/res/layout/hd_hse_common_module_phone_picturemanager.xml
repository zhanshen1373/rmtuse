<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/hd_hse_common_component_phone_content_back"
    android:orientation="vertical"
    android:padding="@dimen/hd_common_plate_gap_horizontal" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <GridView
            android:id="@+id/hd_hse_common_module_phone_photomanager_gv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/hd_common_plate_gap_horizontal"
            android:horizontalSpacing="@dimen/hd_common_plate_gap_horizontal"
            android:numColumns="3"
            android:scrollbars="none"
            android:verticalSpacing="@dimen/hd_common_plate_gap_horizontal" />
    </LinearLayout>

</LinearLayout>