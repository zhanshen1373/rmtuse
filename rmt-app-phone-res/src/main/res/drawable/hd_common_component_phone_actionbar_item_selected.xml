<?xml version="1.0" encoding="utf-8"?>
<!-- 弹出菜单选择效果 -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:drawable="@color/hd_hse_common_component_phone_actionbar_background" android:state_window_focused="false"></item>
    <item android:drawable="@color/hd_hse_common_component_phone_actionbar_item_touch" android:state_selected="true"></item>
    <item android:drawable="@color/hd_hse_common_component_phone_actionbar_item_touch" android:state_focused="false" android:state_pressed="true"></item>
    <item android:drawable="@color/hd_hse_common_component_phone_actionbar_item_touch" android:state_focused="true" android:state_pressed="true"/>
    <item android:drawable="@color/hd_hse_common_component_phone_actionbar_item_touch" android:state_focused="true"/>

</selector>